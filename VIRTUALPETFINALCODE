	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0x9f, 0xf3, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xa7, 0xcb, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfe, 0x2b, 0xa8, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfe, 0xec, 0x6e, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfe, 0xec, 0x6e, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfc, 0xe9, 0x2e, 0x7f, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfd, 0xe5, 0x4f, 0x7f, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfd, 0xfd, 0x7f, 0x7f, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfd, 0xfb, 0xbf, 0x7f, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 
	0xff, 0xff, 0xff, 0xff, 0xff, 0xff, 0xfd, 0xff, 0xff, 0x7f, 0xff, 0xff, 0xff, 0xff, 0xff, 0xff
};

  void setup() {
    display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
    display.display();
    delay(2000);
    display.clearDisplay();
    //so the code knows to start with idle animations
    idleStartTime = millis();
    cute.init(BUZZER_PIN);
    pinMode(BUTTON_PIN, INPUT_PULLUP); //button pin as input with internal pull-up resistor
}

void loop() {
    unsigned long currentTime = millis();
    static bool lastButtonState = HIGH;
    bool buttonState = digitalRead(BUTTON_PIN);

    // Check if the button state has changed
    if (buttonState != lastButtonState) {
        // If the button is pressed (LOW), switch to sleep animation
        if (buttonState == LOW) {
            isIdle = false;
            isAlert = false;
            isCrying = false;
            isMad = false;
            isSleeping = true;
            sleepStartTime = currentTime;
            currentFrame = 0; // Reset frame for sleep animation
        }
    }

    lastButtonState = buttonState;

    // Display animations based on current state
    if (isSleeping) {
        // Sleep animation
        display.clearDisplay();
        if (currentFrame == 0) {
            display.drawBitmap(0, 0, frame21, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else {
            display.drawBitmap(0, 0, frame22, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
            cute.play(S_SLEEPING);
        }
        display.display();
        // Check if it's time to transition out of sleep state
        if (currentTime - sleepStartTime >= SLEEP_DURATION) {
            isSleeping = false;
            isIdle = true;
            idleStartTime = currentTime;
            currentFrame = 0; // Reset frame for idle animation
        }
    }
    // Display animations based on current state
    if (isIdle) {
        // Idle animation
        display.clearDisplay();
        if (currentFrame == 0) {
            display.drawBitmap(0, 0, frame0, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else {
            display.drawBitmap(0, 0, frame1, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        }
        display.display();
        delay(FRAME_DURATION);
        currentFrame = (currentFrame + 1) % NUM_FRAMES_IDLE;

        // Check if it's time to transition out of idle state
        if (currentTime - idleStartTime >= IDLE_DURATION) {
            isIdle = false;
            isAlert = true;
            alertStartTime = currentTime;
            currentFrame = 0; // Reset frame for alert animation
        }
    }

    if (isAlert) {
        // Alert animation
        display.clearDisplay();
        if (currentFrame == 0) {
            display.drawBitmap(0, 0, frame2, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else {
            display.drawBitmap(0, 0, frame3, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
            cute.play(S_SURPRISE);
        }
        display.display();
        delay(FRAME_DURATION);
        currentFrame = (currentFrame + 1) % NUM_FRAMES_ALERT;

        // Check if it's time to transition out of alert state
        if (currentTime - alertStartTime >= ALERT_DURATION) {
            isAlert = false;
            isCrying = true;
            cryingStartTime = currentTime;
            currentFrame = 0; // Reset frame for crying animation
        }
    }

    if (isCrying) {
        // Crying animation
        display.clearDisplay();
        if (currentFrame == 0) {
            display.drawBitmap(0, 0, frame13, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else if (currentFrame == 1) {
            display.drawBitmap(0, 0, frame14, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else if (currentFrame == 2) {
            display.drawBitmap(0, 0, frame15, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else if (currentFrame == 3) {
            display.drawBitmap(0, 0, frame16, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
            cute.play(S_SAD);
        }
        display.display();
        delay(FRAME_DURATION);
        currentFrame = (currentFrame + 1) % NUM_FRAMES_CRYING;

        // Check if it's time to transition out of crying state
        if (currentTime - cryingStartTime >= CRYING_DURATION) {
            isCrying = false;
            isMad = true;
            madStartTime = currentTime;
            currentFrame = 0; // Reset frame for mad animation
        }
    }

    if (isMad) {
        // Mad animation
        display.clearDisplay();
        if (currentFrame == 0) {
            display.drawBitmap(0, 0, frame17, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else if (currentFrame == 1) {
            display.drawBitmap(0, 0, frame18, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else if (currentFrame == 2) {
            display.drawBitmap(0, 0, frame19, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else if (currentFrame == 3) {
            display.drawBitmap(0, 0, frame20, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
            cute.play(S_CONFUSED);
        }
        display.display();
        delay(FRAME_DURATION);
        currentFrame = (currentFrame + 1) % NUM_FRAMES_MAD;

        // Check if it's time to transition out of mad state
        if (currentTime - madStartTime >= MAD_DURATION) {
            isMad = false;
            isIdle = true;
            idleStartTime = currentTime;
            currentFrame = 0; // Reset frame for idle animation
        }
    }

    if (isSleeping) {
        // Sleep animation
        display.clearDisplay();
        if (currentFrame == 0) {
            display.drawBitmap(0, 0, frame21, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
        } else {
            display.drawBitmap(0, 0, frame22, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
            cute.play(S_SLEEPING);
        }
        display.display();
        delay(FRAME_DURATION);
    }
}
